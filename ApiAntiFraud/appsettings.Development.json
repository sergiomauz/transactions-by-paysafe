{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Application": {
    "Timezone": -5,
    "TopCurrentValue": 2500,
    "TopAccumulatedValueByDay": 20500
  },
  "ExternalServices": {
    "KafkaClient": {
      "RemoteHost": "kafka",
      "RemotePort": 9092,
      "LingerMs": 5,
      "AccountTransactionsTopic": "account-transactions",
      "AccountTransactionsTopicFails": "account-transactions-fails",
      "ClientId": "account-transactions-validator-consumer"
    },
    "ApiTransactions": {
      "PublicKey": "Api_Key",
      "PrivateKey": "Private_Key",
      "RemoteBaseUrl": "https://dotnet_api_bank_transactions",
      "RemotePort": 8081,
      "Timeout": 5,
      "Resources": {
        "UpdateTransaction": "/api/account-transactions/{0}"
      }
    }
  }
}
